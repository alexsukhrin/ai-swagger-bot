{
  "name": "ai-swagger-bot-serverless",
  "version": "1.0.0",
  "description": "AI Swagger Bot API - Serverless deployment package",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:staging": "serverless deploy --stage staging",
    "remove": "serverless remove",
    "remove:prod": "serverless remove --stage prod",
    "remove:staging": "serverless remove --stage staging",
    "logs": "serverless logs -f api",
    "logs:prod": "serverless logs -f api --stage prod",
    "logs:staging": "serverless logs -f api --stage staging",
    "invoke": "serverless invoke -f api",
    "invoke:local": "serverless invoke local -f api",
    "test:local": "serverless invoke local -f api --path test-event.json",
    "package": "serverless package",
    "info": "serverless info",
    "info:prod": "serverless info --stage prod",
    "info:staging": "serverless info --stage staging"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "fastapi",
    "mangum",
    "swagger",
    "ai",
    "api"
  ],
  "author": "AI Swagger Bot Team",
  "license": "MIT",
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-python-requirements": "^6.0.0",
    "serverless-offline": "^13.3.0",
    "serverless-dotenv-plugin": "^6.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
} 